@startuml
'https://plantuml.com/class-diagram

class Client{}

interface ServiceInterface {
    + operation()
}

class Proxy {
    - realService: Service
    + Proxy(s: Service)
    + checkAccess()
    + operation()
}

class Service {
    +operation()
}

ServiceInterface <- Client
Proxy o-> Service
ServiceInterface<|..Proxy
ServiceInterface<|..Service

@enduml